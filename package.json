{
	"name": "email-validation",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"clean": "rm -rf .next && rm -rf out",
		"build": "npm run clean && next build",
		"start": "serve out",
		"preexport": "npm run build",
		"export": "next export",
		"prestart": "npm run export",
		"prepare": "cd .. && husky install ./email-validation/.husky",
		"test:unit": "jest ./email-validation/src/tests --collect-coverage --env=jsdom"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"bootstrap": "^5.1.1",
		"ga-4-react": "^0.1.281",
		"jquery": "^3.6.0",
		"mongodb": "^4.1.2",
		"next": "^10.2.3",
		"next-connect": "^0.10.2",
		"postcss": "^8.3.0",
		"react": "^17.0.2",
		"react-awesome-slider": "^4.1.0",
		"react-bootstrap": "^2.0.0-rc.0",
		"react-dom": "^17.0.2",
		"react-router-dom": "^5.3.0"
	},
	"devDependencies": {
		"@testing-library/dom": "^8.0.0",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@testing-library/user-event": "^13.1.9",
		"@types/react": "17.0.8",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"axios-mock-adapter": "^1.19.0",
		"babel-jest": "^27.0.6",
		"eslint": "^7.27.0",
		"eslint-plugin-react": "^7.24.0",
		"husky": "^6.0.0",
		"jest": "^27.0.6",
		"jest-dom": "^4.0.0",
		"lint-staged": "^7.3.0",
		"prettier": "^2.3.0",
		"serve": "^11.3.2",
		"ts-jest": "^27.0.3",
		"ts-node": "^10.0.0",
		"typescript": "4.3.2",
		"url-loader": "^4.1.1"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		],
		"../common/*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"git add"
		],
		"*.{html,css,less,ejs}": [
			"prettier --write",
			"git add"
		],
		"*.js": "eslint --cache --fix",
		"*.{js,css,md}": "prettier --write"
	}
}
